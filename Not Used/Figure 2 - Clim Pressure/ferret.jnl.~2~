use "http://www.esrl.noaa.gov/psd/thredds/dodsC/Datasets/ncep.reanalysis.derived/surface/pres.mon.mean.nc"
show data
cancel data 2
use "http://www.esrl.noaa.gov/psd/thredds/dodsC/Datasets/ncep.reanalysis.derived/surface/slp.mon.mean.nc"
use "ds010.1.19970100.20111231"
use "ds010.1.19970100.20111231"
show data
list SLP[I=1,J=1,L=1]
list SLP[I=1,J=1:10,L=1]
list SLP[I=1,L=1]
list SLP[J=1,L=1]
show data
shade SLP[d=3,L=1]
shade SLP[d=3,L=1:3@AVE]
shade SLP[d=3,X=160E:140W,Y=30N:65NL=1:3@AVE]
shade SLP[d=3,X=160E:140W,Y=30N:65N,L=1:3@AVE]
list SLP[d=2,X=160E,Y=50N,L=100:110]
list SLP[d=2,X=160E,Y=50N,L=400:410]
list SLP[d=2,X=160E,Y=50N,L=550:560]
list SLP[d=2,X=160E,Y=50N,L=600:610]
list SLP[d=2,X=160E,Y=50N,L=593:610]
list SLP[d=2,X=160E,Y=50N,L=596:610]
list SLP[d=2,X=160E,Y=50N,L=599:610]
list SLP[d=2,X=160E,Y=50N,L=590:610]
