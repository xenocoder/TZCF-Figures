shade/spacing=0.1 SLP[X=160E:140W:0.1,Y=30N:65N:0.1,L=589:610@AVE]
shade SLP[X=160E:140W:0.1,Y=30N:65N:0.1,L=589:610@AVE]
show data
shade SLP[d=2,X=160E:140W:0.1,Y=30N:65N:0.1,L=589:610@AVE]
let slp-tzcf = SLP[X=160E:140W:0.1,Y=30N:65N:0.1,T="15-JAN-1997":"15-DEC-2011"]
let slptzcf = SLP[X=160E:140W:0.1,Y=30N:65N:0.1,T="15-JAN-1997":"15-DEC-2011"]
list/format=CDF/file=slptzcf.nc slptzcf
set memory/size=1000M
set memory/size=1000
list/format=CDF/file=slptzcf.nc slptzcf
let slptzcf = SLP[X=160E:140W,Y=30N:65N,T="15-JAN-1997":"15-DEC-2011"]
list/format=CDF/file=slptzcf.nc slptzcf
let slptzcf = SLP[USE climatological_axesX=160E:140W,Y=30N:65N,T="15-JAN-1997":"15-DEC-2011"]
USE climatological_axes
show data
let slpclimncep = SLP[d=2,X=160E:140W,Y=30N:65N,T="15-JAN-1997":"15-DEC-2011"]
let slpclimncar = SLP[d=3,X=160E:140W,Y=30N:65N,T="15-JAN-1997":"15-DEC-2011"]
shade slpclimncep[L=1]
set region/X=160E:140W/Y=30N:65N/T="15-JAN-1997":"15-DEC-2011"
list SLP[d=2,i=1,j=1,l=1]
