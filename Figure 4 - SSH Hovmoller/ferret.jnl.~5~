 ! NOAA/PMEL TMAP
 ! FERRET v6.3  
 ! Darwin 9.8.0 (ifort) - 11/30/09
 !  9-Mar-14 19:49     

use ncep-npi-slp-monmean.nc
show data
shade/levels=(-0.4,0.4,0.04) SSHD[d=1,Y=32N:34N@AVE,L=1:180]
shade SLP[d=1,Y=32N:34N@AVE,L=1:180]
shade SLP[d=2,Y=32N:34N@AVE,L=1:180]
shade SLP[Y=32N:34N@AVE,L=1:180]
shade SLPNCEP[d=1,Y=32N:34N@AVE,L=1:180]
use "http://oceanwatch.pifsc.noaa.gov/thredds/dodsC/altim_merged/monthly"
show data
let sshnpac = SSH[d=2,X=160E:140W,Y=32N:34N,T="15-JAN-1997":"15-DEC-2011"]
shade sshnpac[X=@SBX:10,Y=@AVE,T=@SBX:12]
let sshnpacanom = sshnpac - sshnpac[T=@AVE]
shade sshnpacanom[X=@SBX:10,Y=@AVE,T=@SBX:12]
shade/levels=(-25,25,5) sshnpacanom[X=@SBX:10,Y=@AVE,T=@SBX:12]
shade/levels=(-25,25,2.5) sshnpacanom[X=@SBX:10,Y=@AVE,T=@SBX:12]
contour/over/levels=(-25,25,2.5) sshnpacanom[X=@SBX:10,Y=@AVE,T=@SBX:12]
list/format=cdf/file=tzcf-aviso-ssh-hov.nc sshnpacanom
set mode metafile
shade/levels=(-25,25,2.5) sshnpacanom[X=@SBX:10,Y=@AVE,T=@SBX:12]
contour/over/levels=(-25,25,2.5) sshnpacanom[X=@SBX:10,Y=@AVE,T=@SBX:12]
cancel model metafile
PPL CLSPLT
set mode metafile
plot/1:10
PLOT/i=1:10  1/i
cancel mode metafile
PPL CLSPLT
w
