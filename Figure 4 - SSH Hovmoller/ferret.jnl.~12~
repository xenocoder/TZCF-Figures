use aviso-20090318.nc
use aviso-20110316.nc
show data
shade/PALETTE=blue_darkred/levels=(-20,20,1) GRID_0001[d=1,x=198:208:0.01,y=25.5:36.5:0.01]
contour/over/levels=c(-20,20,2) GRID_0001[d=1,x=198:208:0.01,y=25.5:36.5:0.01]
DEFINE VIEWPORT/XLIM=0,0.5/YLIM=0,1 wleft
DEFINE VIEWPORT/XLIM=0.5,1/YLIM=0,1 wright
set window/aspect=2
shade/PALETTE=blue_darkred/levels=(-20,20,1) GRID_0001[d=1,x=198:208:0.01,y=25.5:36.5:0.01]
set window/aspect=2.75
shade/PALETTE=blue_darkred/levels=(-20,20,1) GRID_0001[d=1,x=198:208:0.01,y=25.5:36.5:0.01]
set viewport wleft
shade/PALETTE=blue_darkred/levels=(-20,20,1) GRID_0001[d=1,x=198:208:0.01,y=25.5:36.5:0.01]
set window/aspect=1
shade/PALETTE=blue_darkred/levels=(-20,20,1) GRID_0001[d=1,x=198:208:0.01,y=25.5:36.5:0.01]
set viewport wleft
shade/PALETTE=blue_darkred/levels=(-20,20,1) GRID_0001[d=1,x=198:208:0.01,y=25.5:36.5:0.01]
use tzcf-aviso-ssh-hov.nc
show data
list SSHNPAC[d=3,i=1,j=1,l=1]
list SSHNPACANOM[d=3,i=1,j=1,l=1]
list SSHNPACANOM[d=3,i=1,j=1,l=180]
exit
